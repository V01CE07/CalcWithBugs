<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="350" Height="450" CanResize="False"
			Icon=""
  WindowStartupLocation="CenterScreen"
        x:Class="EngineCalcDrozdov.MainWindow"
        Title="EngineCalc">
				

	<Window.Styles>
		<Styles>
			<Style Selector="Button:pointerover /template/ ContentPresenter">
				<Setter Property="Background" Value="#b59c57">
				</Setter>
			</Style>
		</Styles>
	</Window.Styles>

	<Grid RowDefinitions="150, *,  150" Background="#42413e"
		ColumnDefinitions="0, 50, 50, 50, 50, 50, 50"
		
	ShowGridLines="False">


		<Button
		 Click="clickNumber"
		 Name="zero"
		 Grid.Row="3"
		 Grid.Column="4"
		 HorizontalAlignment="Stretch"
		 VerticalAlignment="Bottom"
		 Height="50"
		 Width=""
		 HorizontalContentAlignment="Center"
		 VerticalContentAlignment="Center"
		 Content="0"
		 FontSize="18"
		 FontWeight="Light"
		 Background="#e6b01c"
		 />
		
		<Button
		Click="clickDecimal"
		Name="dec"
		Grid.Row="3"
		Grid.Column="5"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Bottom"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content=","
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="pi"
		Grid.Row="3"
		Grid.Column="3"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Bottom"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="π"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

