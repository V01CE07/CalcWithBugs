		Grid.Column="1"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Bottom"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="^(-1)"
		FontSize="17"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="factorial"
		Grid.Row="3"
		Grid.Column="1"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Center"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="x!"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="mathRoot"
		Grid.Row="3"
		Grid.Column="1"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Top"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="√x"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="positiveDegree"
