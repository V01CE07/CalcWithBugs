		Content="←"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="percent"
		Grid.Row="1"
		Grid.Column="5"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Center"
		Height="50"
		Width=""
