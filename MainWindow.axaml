		Click="clickNumber"
		Name="one"
		Grid.Row="3"
		Grid.Column="3"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Center"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="1"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickNumber"
		Name="two"
		Grid.Row="3"
		Grid.Column="4"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Center"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="2"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickNumber"
		Name="three"	
