		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="div"
		Grid.Row="1"
		Grid.Column="2"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Bottom"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="÷"
		FontSize="18"
		FontWeight="DemiBold"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="plus"
		Grid.Row="2"
		Grid.Column="2"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Bottom"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
		VerticalContentAlignment="Center"
		Content="+"
		FontSize="18"
		FontWeight="DemiBold"
		Background="#e6b01c"
		/>

		<Button
		Click="clickSkobkaRight"
