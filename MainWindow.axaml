		FontSize="17"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickOperations"
		Name="tg"
		Grid.Row="1"
		Grid.Column="3"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Top"
		Height="50"
		Width=""
		HorizontalContentAlignment="Center"
