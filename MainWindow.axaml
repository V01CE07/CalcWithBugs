		VerticalContentAlignment="Center"
		Content="3"
		FontSize="18"
		FontWeight="Light"
		Background="#e6b01c"
		/>

		<Button
		Click="clickNumber"
		Name="four"
		Grid.Row="3"
		Grid.Column="3"
		HorizontalAlignment="Stretch"
