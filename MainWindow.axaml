		<TextBlock
		Name="inputCalc"
		Grid.Row="0"
		Grid.Column="1"
		Text=""
		HorizontalAlignment="Left"
		VerticalAlignment="Center"
		Width="10000"
		FontSize="22"
		FontWeight="Light"
		Foreground="White"
		LetterSpacing="1">
		</TextBlock>

		<TextBlock
		Name="outputCalc"
		Grid.Row="0"
		Grid.Column="1"
		Text=""
		Margin="0, -60"
		HorizontalAlignment="Left"
		VerticalAlignment="Center"
		Width="1000"
		FontSize="16"
		FontWeight="Light"
		Foreground="White"
		LetterSpacing="1">
		</TextBlock>
		
		<Polyline
